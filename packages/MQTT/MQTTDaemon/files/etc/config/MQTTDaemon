config default 'MQTTDaemon'
	option Address 'mqtt.hyhsystem.cn:1883'
	option Keepalive '120'
	option ClientId ''
	option UserName ''
	option PassWord ''
	option SSL '0'
	option CAFile '/etc/MQTTDaemon/emqx/cacert.pem'
	option CAPath '/etc/MQTTDaemon/emqx/'
	option CertFile '/etc/MQTTDaemon/emqx/client-cert.pem'
	option KeyFile '/etc/MQTTDaemon/emqx/client-key.pem'
	option CheckServerName '0'
